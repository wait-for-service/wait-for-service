{
    "branches": [
        {
            "name": "main",
            "prerelease": false
        },
        {
            "name": "releases\\/+([0-9])?(\\.\\d+)(\\.\\d+|z|$)",
            "prerelease": false
        },
        {
            "name": "next",
            "prerelease": false
        },
        {
            "name": "next-major",
            "prerelease": true
        }
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "pom.xml",
                        "label": "pom"
                    },
                    {
                        "path": "target/s3proxy",
                        "label": "s3proxy"
                    }
                ]
            }
        ]
    ]
}
